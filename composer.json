{
    "name": "bolt/common",
    "description": "Common utilities for all Bolt libraries",
    "license": "MIT",
    "authors": [
        {
            "name": "Carson Full",
            "email": "carsonfull@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/phpunit-bridge": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Bolt\\Common\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Common\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
